{
  "name": "hello-world",
  "version": "1.0.0",
  "description": "Live demo of MVPKit - a modern full-stack TypeScript starter with React, tRPC, Cloudflare Workers, and authentication. Build and deploy MVPs faster.",
  "private": false,
  "type": "module",
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "deploy:apps": "turbo run deploy:app",
    "deploy:backend": "turbo run deploy:app --filter=@repo/api",
    "deploy:frontend": "turbo run deploy:app --filter=@repo/app",
    "lint": "turbo lint",
    "lint:fix": "turbo run lint:fix",
    "format": "turbo format",
    "format:fix": "turbo run format:fix",
    "typecheck": "turbo typecheck",
    "clean": "turbo clean"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@types/fs-extra": "11.0.4",
    "concurrently": "9.2.1",
    "turbo": "2.5.6",
    "typescript": "5.9.2"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0",
    "bun": ">=1.0.0"
  },
  "packageManager": "pnpm@10.14.0",
  "workspaces": [
    "apps/*",
    "packages/*",
    "services/*"
  ],
  "pnpm": {
    "overrides": {}
  }
}
